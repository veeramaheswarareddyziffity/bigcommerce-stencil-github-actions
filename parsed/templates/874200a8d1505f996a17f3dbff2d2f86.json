{
  "components/cart/totals": "<ul class=\"cart-totals {{#if cart.multi_coupon_ui_enabled}}cart-total-enhanced{{/if}}\">\n    <li class=\"cart-total\">\n        <div class=\"cart-total-label\">\n            <strong>{{lang 'cart.checkout.subtotal'}}{{#unless cart.multi_coupon_ui_enabled}}:{{/unless}}</strong>\n        </div>\n        <div class=\"cart-total-value\">\n            {{#or customer (unless settings.hide_price_from_guests)}}\n                <span>{{cart.sub_total.formatted}}</span>\n            {{else}}\n                {{> components/common/login-for-pricing}}\n            {{/or}}\n        </div>\n    </li>\n    {{#if cart.multi_coupon_ui_enabled}}\n        {{#or cart.coupons cart.display_discount_total.value}}\n            <div class=\"cart-total cart-total-discounts\">\n                <div class=\"cart-total-label\" data-discount-toggle>\n                    <strong>{{lang 'cart.discounts'}}</strong>\n                    <i class=\"icon cart-discount-icon\" aria-hidden=\"true\"><svg><use href=\"#icon-keyboard-arrow-up\" /></svg></i>\n                </div>\n                <div class=\"cart-total-value\" data-discount-toggle>\n                    <span>{{cart.display_discount_total.formatted}}</span>\n                </div>\n                <ul data-discount-details class=\"cart-discount-list\">\n                    {{#if cart.order_based_auto_discount_total.value}}\n                        <li class=\"cart-discount-item\">\n                            <div class=\"cart-discount-item-label\">\n                                <i class=\"icon\" aria-hidden=\"true\"><svg><use href=\"#icon-promotions\" /></svg></i>\n                                <span class=\"cart-coupon-name\">{{lang 'cart.automatic_discount'}}</span>\n                            </div>\n                            <div class=\"cart-discount-item-value\">\n                                {{cart.order_based_auto_discount_total.formatted}}\n                            </div>\n                        </li>\n                    {{/if}}\n                    {{#if cart.manual_discount_total.value}}\n                        <li class=\"cart-discount-item\">\n                            <div class=\"cart-discount-item-label\">\n                                <i class=\"icon\" aria-hidden=\"true\"><svg><use href=\"#icon-promotions\" /></svg></i>\n                                <span class=\"cart-coupon-name\">{{lang 'cart.manual_discount'}}</span>\n                            </div>\n                            <div class=\"cart-discount-item-value\">\n                                {{cart.manual_discount_total.formatted}}\n                            </div>\n                        </li>\n                    {{/if}}\n                    {{#each cart.coupons}}\n                        <li class=\"cart-discount-item\">\n                            <div class=\"cart-discount-item-label\">\n                                <i class=\"icon\" aria-hidden=\"true\"><svg><use href=\"#icon-promotions\" /></svg></i>\n                                {{#if display_name}}\n                                    <span class=\"cart-coupon-name\">{{display_name}} ({{code}})</span>\n                                {{else}}\n                                    <span class=\"cart-coupon-name\">{{code}}</span>\n                                {{/if}}\n                            </div>\n                            <div class=\"cart-discount-item-value\">\n                                {{#if discount.value '===' 0}}-{{/if}}{{discount.formatted}}\n                            </div>\n                        </li>\n                    {{/each}}\n                </ul>\n            </div>\n        {{/or}}\n    {{/if}}\n    {{#if cart.gift_wrapping_cost.value}}\n        <li class=\"cart-total\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.checkout.gift_wrapping'}}{{#unless cart.multi_coupon_ui_enabled}}:{{/unless}}</strong>\n            </div>\n            <div class=\"cart-total-value\">\n                <span>{{cart.gift_wrapping_cost.formatted}}</span>\n            </div>\n        </li>\n    {{/if}}\n    {{#if cart.fees}}\n        {{#each cart.fees}}\n            <li class=\"cart-total\">\n                <div class=\"cart-total-label\">\n                    <strong>{{display_name}}{{#unless ../cart.multi_coupon_ui_enabled}}:{{/unless}}</strong>\n                </div>\n                <div class=\"cart-total-value\">\n                    <span>{{cost.formatted}}</span>\n                </div>\n            </li>\n        {{/each}}\n    {{/if}}\n    {{#if cart.shipping_handling.show_estimator}}\n        <li class=\"cart-total {{#if cart.multi_coupon_ui_enabled}}cart-total-shipping-estimator{{/if}}\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.checkout.shipping'}}{{#unless cart.multi_coupon_ui_enabled}}:{{/unless}}</strong>\n                {{#if cart.multi_coupon_ui_enabled}}\n                    {{> components/cart/shipping-estimator-trigger}}\n                {{/if}}\n            </div>\n            {{#neither cart.multi_coupon_ui_enabled cart.shipping_handling.shipping_cost}}\n                {{> components/cart/shipping-estimator-trigger}}\n            {{/neither}}\n            {{> components/cart/shipping-estimator cart.shipping_handling multi_coupon_ui_enabled=cart.multi_coupon_ui_enabled}}\n        </li>\n    {{/if}}\n    {{#each cart.taxes}}\n        {{#unless included}}\n            <li class=\"cart-total\">\n                <div class=\"cart-total-label\">\n                    <strong>{{name}}{{#unless ../cart.multi_coupon_ui_enabled}}:{{/unless}}</strong>\n                </div>\n                <div class=\"cart-total-value\">\n                    <span>{{cost.formatted}}</span>\n                </div>\n            </li>\n        {{/unless}}\n    {{/each}}\n    {{#unless cart.multi_coupon_ui_enabled}}\n        {{#if cart.discount }}\n            <li class=\"cart-total\">\n                <div class=\"cart-total-label\">\n                    <strong>{{lang 'cart.discount'}}:</strong>\n                </div>\n                <div class=\"cart-total-value\">\n                    {{cart.discount.formatted}}\n                </div>\n            </li>\n        {{/if}}\n        {{#if cart.coupons}}\n            {{#each cart.coupons}}\n                <li class=\"cart-total\">\n                    <div class=\"cart-total-label\">\n                        <strong>{{lang 'cart.coupons.code_label' code=code}}</strong>\n                        <div>\n                            <a href=\"{{remove_url}}\">{{lang 'cart.coupons.remove'}}</a>\n                        </div>\n                    </div>\n                    <div class=\"cart-total-value\">\n                        <span>{{discount.formatted}}</span>\n                    </div>\n                </li>\n            {{/each}}\n        {{else}}\n            <li class=\"cart-total\">\n                <div class=\"cart-total-label\">\n                    <strong>{{lang 'cart.coupon_code'}}:</strong>\n                </div>\n                {{> components/cart/coupon-input}}\n            </li>\n        {{/if}}\n    {{/unless}}\n    {{#if settings.gift_certificates_enabled}}\n        {{#unless cart.multi_coupon_ui_enabled}}\n            {{#each cart.gift_certificates}}\n                <li class=\"cart-total\">\n                    <div class=\"cart-total-label\">\n                        <strong>{{lang 'cart.gift_certificates.code_label' code=code}}</strong>\n                        <span>\n                            {{remaining.formatted}} | <a href=\"{{remove_url}}\">{{lang 'cart.gift_certificates.remove'}}</a>\n                        </span>\n                    </div>\n                    <div class=\"cart-total-value\">\n                        {{used.formatted}}\n                    </div>\n                </li>\n            {{/each}}\n        {{/unless}}\n        <li class=\"cart-total {{#if cart.multi_coupon_ui_enabled}}cart-total-gift-certificate{{/if}}\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.gift_certificates.gift_certificate'}}{{#unless cart.multi_coupon_ui_enabled}}:{{/unless}}</strong>\n                {{#if cart.multi_coupon_ui_enabled}}\n                    {{> components/cart/gift-certificate}}\n                {{/if}}\n            </div>\n            {{#unless cart.multi_coupon_ui_enabled}}\n                {{> components/cart/gift-certificate}}\n            {{/unless}}\n            {{> components/cart/gift-certificate-input}}\n        </li>\n        {{#if cart.multi_coupon_ui_enabled}}\n            {{#each cart.gift_certificates}}\n                <li class=\"cart-total cart-total-gift-certificate-list-item\">\n                    <div class=\"cart-total-label\">\n                        <i class=\"icon\" aria-hidden=\"true\"><svg><use href=\"#icon-gifts\" /></svg></i>\n                        <span>{{code}}</span>\n                        {{#if remaining.value}}\n                            <span>| {{remaining.formatted}} left</span>\n                        {{/if}}\n                        <a href=\"{{remove_url}}\">{{lang 'cart.gift_certificates.remove'}}</a>\n                    </div>\n                    <div class=\"cart-total-value\">\n                        {{used.formatted}}\n                    </div>\n                </li>\n            {{/each}}\n        {{/if}}\n    {{/if}}\n    <li class=\"cart-total {{#if cart.multi_coupon_ui_enabled}}cart-total-with-savings{{/if}}\">\n        {{#if cart.multi_coupon_ui_enabled}}\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.checkout.total'}}</strong>\n            </div>\n            <div class=\"cart-total-value cart-total-grandTotal\">\n                {{#or customer (unless settings.hide_price_from_guests) }}\n                    <span class=\"cart-total-amount\">{{cart.grand_total.formatted}}</span>\n                    {{#if cart.total_discount.value}}\n                        <span class=\"cart-total-savings\">{{lang 'cart.checkout.you_saved'}} <span class=\"cart-savings-amount\">{{cart.total_discount.formatted}}</span> {{lang 'cart.checkout.in_total'}}</span>\n                    {{/if}}\n                {{else}}\n                    {{> components/common/login-for-pricing}}\n                {{/or}}\n            </div>\n        {{else}}\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.checkout.grand_total'}}:</strong>\n            </div>\n            <div class=\"cart-total-value cart-total-grandTotal\">\n                {{#or customer (unless settings.hide_price_from_guests)}}\n                    <span>{{cart.grand_total.formatted}}</span>\n                {{else}}\n                    {{> components/common/login-for-pricing}}\n                {{/or}}\n            </div>\n        {{/if}}\n    </li>\n    {{#each cart.taxes}}\n        {{#if included}}\n            <li class=\"cart-total\">\n                <div class=\"cart-total-label\">\n                    <strong>{{concat name (lang 'cart.included_in_total')}}{{#unless ../cart.multi_coupon_ui_enabled}}:{{/unless}}</strong>\n                </div>\n                <div class=\"cart-total-value\">\n                    <span>{{cost.formatted}}</span>\n                </div>\n            </li>\n        {{/if}}\n    {{/each}}\n</ul>\n",
  "components/cart/shipping-estimator-trigger": "<div class=\"{{#unless cart.multi_coupon_ui_enabled}}cart-total-value{{/unless}}\">\n    <button data-collapsible=\"add-shipping\" class=\"shipping-estimate-show\" aria-labelledby=\"estimator-add\">\n        <span class=\"shipping-estimate-show__btn-name\">\n            {{#if cart.shipping_handling.shipping_cost}}\n                {{lang 'cart.gift_wrapping.change'}}\n            {{else}}\n                {{#if cart.multi_coupon_ui_enabled}}\n                    {{lang 'cart.gift_wrapping.add'}}\n                {{else}}\n                    {{lang 'cart.shipping_estimator.add_info'}}\n                {{/if}}\n            {{/if}}\n        </span>\n        <span id=\"estimator-add\" class=\"u-hidden\">\n            {{#if cart.shipping_handling.shipping_cost}}\n                {{lang 'cart.gift_wrapping.change'}}\n            {{else}}\n                {{#if cart.multi_coupon_ui_enabled}}\n                    {{lang 'cart.gift_wrapping.add'}}\n                {{else}}\n                    {{lang 'cart.shipping_estimator.add_info'}}\n                {{/if}}\n            {{/if}}\n        </span>\n        <span id=\"estimator-close\" class=\"u-hidden\">{{lang 'cart.shipping_estimator.cancel'}}</span>\n    </button>\n</div>\n",
  "components/cart/shipping-estimator": "{{inject 'shippingCountryErrorMessage' (lang 'cart.shipping_estimator.empty_country_error')}}\n{{inject 'shippingProvinceErrorMessage' (lang 'cart.shipping_estimator.empty_province_error')}}\n\n{{#if shipping_cost}}\n    <div class=\"cart-total-value\">\n        <div class=\"subtotal{{#unless multi_coupon_ui_enabled}} shipping-estimate-show{{/unless}}\">\n            {{#if multi_coupon_ui_enabled}}\n                {{#and shipping_cost_discounted (if shipping_cost_discounted.formatted '!==' shipping_cost.formatted)}}\n                    <span class=\"price--discounted\">{{shipping_cost.formatted}}</span>\n                    <span id=\"shipping-cost-discounted\">{{shipping_cost_discounted.formatted}}</span>\n                {{else}}\n                    <span id=\"shipping-cost-discounted\">{{shipping_cost.formatted}}</span>\n                {{/and}}\n            {{else}}\n                <a href=\"#\" class=\"shipping-estimate-value\">\n                    <span class=\"{{#if shipping_cost_discounted}}price--discounted{{/if}}\">{{shipping_cost.formatted}}</span>\n                    {{#if shipping_cost_discounted}}\n                        <span id=\"shipping-cost-discounted\">{{shipping_cost_discounted.formatted}}</span>\n                    {{/if}}\n                </a>\n            {{/if}}\n        </div>\n    </div>\n{{/if}}\n\n<div id=\"add-shipping\" class=\"shipping-estimator u-hidden\">\n    <form class=\"form estimator-form\" data-shipping-estimator>\n        <dl>\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-country\">{{lang 'cart.shipping_estimator.select_a_country'}}</label>\n            </dt>\n\n            <dd class=\"estimator-form-input\">\n                <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\n                    <option>{{lang 'cart.shipping_estimator.select_a_country'}}</option>\n                    {{#each countries}}\n                        <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                            {{name}}\n                        </option>\n                    {{/each}}\n                </select>\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-state\">{{lang 'cart.shipping_estimator.select_a_state'}}</label>\n            </dt>\n\n            <dd class=\"estimator-form-input\">\n                {{#if states.length}}\n                    <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\n                        <option>{{lang 'cart.shipping_estimator.select_a_state'}}</option>\n                        {{#each states}}\n                            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                                {{name}}\n                            </option>\n                        {{/each}}\n                    </select>\n                {{else}}\n                    <input class=\"form-input\" type=\"text\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" placeholder=\"{{lang 'cart.shipping_estimator.select_a_state'}}\">\n                {{/if}}\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-city\">{{lang 'cart.shipping_estimator.suburb_city'}}</label>\n            </dt>\n            <dd class=\"estimator-form-input\">\n                <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\" placeholder=\"{{lang 'cart.shipping_estimator.suburb_city'}}\">\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-zip\">{{lang 'cart.shipping_estimator.zip_postal_code'}}</label>\n            </dt>\n            <dd class=\"estimator-form-input\">\n                <input autocomplete=\"postal-code\" class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_estimator.zip_postal_code'}}\">\n            </dd>\n            <button class=\"button button--primary button--small shipping-estimate-submit\">{{lang 'cart.shipping_estimator.estimate_shipping'}}</button>\n        </dl>\n    </form>\n    <div class=\"shipping-quotes\" role=\"alert\" aria-atomic=\"true\" aria-live=\"assertive\"></div>\n</div>\n",
  "components/common/login-for-pricing": "<p translate>{{lang 'common.login_for_pricing'}}</p>\n",
  "components/cart/gift-certificate": "<div class=\"{{#unless cart.multi_coupon_ui_enabled}}cart-total-value{{/unless}}\">\n    <button\n        class=\"gift-certificate-add\"\n        aria-label=\"{{lang 'cart.gift_certificates.add_cert_code'}}\"\n        aria-controls=\"add-certificate\"\n        aria-expanded=\"false\"\n    >\n        {{#if cart.multi_coupon_ui_enabled}}\n            {{lang 'cart.gift_wrapping.add'}}\n        {{else}}\n            {{lang 'cart.gift_certificates.gift_certificate'}}\n        {{/if}}\n    </button>\n    <button\n        class=\"gift-certificate-cancel\"\n        style=\"display: none;\"\n        aria-label=\"{{lang 'cart.gift_certificates.add_cert_code'}}\"\n        aria-controls=\"add-certificate\"\n        aria-expanded=\"true\"\n    >\n        {{lang 'cart.coupons.cancel'}}\n    </button>\n</div>\n",
  "components/cart/coupon-input": "<div class=\"cart-total-value\">\n    <button\n        class=\"coupon-code-add\"\n        aria-label=\"{{lang 'cart.coupons.add_coupon'}}\"\n        aria-controls=\"add-coupon\"\n        aria-expanded=\"false\"\n    >\n        {{lang 'cart.coupons.add_coupon'}}\n    </button>\n\n    <button\n        class=\"coupon-code-cancel\"\n        style=\"display: none;\"\n        aria-label=\"{{lang 'cart.coupons.add_coupon'}}\"\n        aria-controls=\"add-coupon\"\n        aria-expanded=\"true\"\n    >\n        {{lang 'cart.coupons.cancel'}}\n    </button>\n</div>\n\n<div id=\"add-coupon\" class=\"cart-form coupon-code\" aria-hidden=\"true\" style=\"display: none;\">\n    <form class=\"form form--hiddenLabels coupon-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"couponcode\">{{lang 'cart.coupons.coupon_code'}}</label>\n        <input class=\"form-input\" data-error=\"{{lang 'cart.coupons.empty_error'}}\"  id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"{{lang 'cart.coupons.coupon_code'}}\">\n        <input class=\"button button--primary button--small\" type=\"submit\" aria-label=\"{{lang 'cart.coupons.apply_coupon'}}\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n",
  "components/cart/gift-certificate-input": "<div id=\"add-certificate\" class=\"cart-form gift-certificate-code\" aria-hidden=\"true\" style=\"display: none;\">\n    <form class=\"form form--hiddenLabels cart-gift-certificate-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"certcode\">{{lang 'cart.gift_certificates.cert_code'}}</label>\n        <input class=\"form-input\" id=\"certcode\" type=\"text\" name=\"certcode\" value=\"\" placeholder=\"{{lang 'cart.gift_certificates.add_cert_code'}}\">\n        <input class=\"button button--primary button--small\" aria-label=\"{{lang 'cart.gift_certificates.apply_gift_certificate'}}\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n"
}
